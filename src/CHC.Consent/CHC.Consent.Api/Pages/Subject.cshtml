@page "{studyId:long}/subject/{subjectIdentifier}"

@model Subject

<table class="table">
    <thead>
    <tr>
        <td>Date Given</td>
        <td>Given For</td>
    </tr>
    </thead>
    <tbody>

    @foreach (var consent in Model.ActiveConsents)
    {
        <tr>
            <td>@consent.DateGiven.ToLongDateString()</td>
        </tr>
    }
    </tbody>
</table>